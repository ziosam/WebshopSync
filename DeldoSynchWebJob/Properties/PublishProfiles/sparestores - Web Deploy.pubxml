<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <PublishProvider>AzureWebSite</PublishProvider>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>http://sparestores.azurewebsites.net</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>False</LaunchSiteAfterPublish>
    <MSDeployServiceURL>sparestores.scm.azurewebsites.net:443</MSDeployServiceURL>
    <DeployIisAppPath>sparestores</DeployIisAppPath>
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <UserName>$sparestores</UserName>
    <_SavePWD>True</_SavePWD>
    <_DestinationType>AzureWebSite</_DestinationType>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="DBModel" Order="1" Enabled="False">
          <Destination Path="Data Source=tcp:sparestores.database.windows.net,1433;Initial Catalog=SpareStores;User ID=spares@sparestores;Password=Stores2016" />
          <Object Type="DbCodeFirst">
            <Source Path="DBMigration" DbContext="FS.SpareStores.DAL.Model.DBModel, FS.SpareStores.DAL" MigrationConfiguration="FS.SpareStores.DAL.Migrations.Configuration, FS.SpareStores.DAL" Origin="Convention" />
          </Object>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)DBModel-Web.config Connection String">
      <ParameterValue>Data Source=tcp:sparestores.database.windows.net,1433;Initial Catalog=SpareStores;User ID=spares@sparestores;Password=Stores2016</ParameterValue>
    </MSDeployParameterValue>
  </ItemGroup>
  <ItemGroup>
    <_ConnectionStringsToInsert Include="DBModel" />
  </ItemGroup>
</Project>